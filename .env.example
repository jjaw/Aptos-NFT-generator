# Aptos Network Configuration
VITE_APP_NETWORK=testnet

# Module address where the contract is deployed
VITE_MODULE_ADDRESS=0x123...

# Aptos API Key (optional - for rate limiting)
VITE_APTOS_API_KEY=your_api_key_here

# Shared Collection Address (optional - will be calculated if not provided)
# This is the deterministic address where the shared collection is stored
# Calculated as: Account.getResourceAccountAddress(MODULE_ADDRESS, "shared_collection_v1")
VITE_SHARED_COLLECTION_ADDRESS=0x456...

# Production Example:
# VITE_APP_NETWORK=mainnet
# VITE_MODULE_ADDRESS=0xYOUR_DEPLOYED_CONTRACT_ADDRESS
# VITE_APTOS_API_KEY=your_production_api_key
# VITE_SHARED_COLLECTION_ADDRESS=0xCALCULATED_SHARED_COLLECTION_ADDRESS